
                ifndef _STRUCT_OBJECT_DEFAULT_SETTINGS_
                define _STRUCT_OBJECT_DEFAULT_SETTINGS_

                struct FObjectDefaultSettings                                   ; настройки объекта по умолчанию                            [8 байт]
; -----------------------------------------
;      7    6    5    4    3    2    1    0
;   +----+----+----+----+----+----+----+----+
;   | A1 | A0 | .. | .. | .. | .. | .. | .. |
;   +----+----+----+----+----+----+----+----+
;
;   A1,A0   [7.,6]  - принадлежность
;                       00 - объект не может кому либо принадлежать
;                       10 - объект может принадлежать кому либо
;                       11 - объект можно подобрать
; -----------------------------------------
Flags           DB #00                                                          ; флаги                                                     [1 байт]
Dummy           DS 7, 0                                                         ; резерв                                                    [7 байт]
                ends

                endif ; ~_STRUCT_OBJECT_DEFAULT_SETTINGS_
